{{#each products as |product|}}

  <div class="productInfo" data-id="{{product.id}}">
    <img src= {{product.image}}>
    <div class="productName">
      Name: {{product.name}}
    </div>
    <div class="productPrice">
      Price: {{product.price}}
    </div>

    <button class="btn btn-primary" id="addProductBtn" data-id="{{product.id}}">Add to cart</button>
  </div>
{{/each}}
